      section(data-bespoke-hash='Desain1')(data-bespoke-state='emphatic')
        div.desain
          div.title
            h2 <img src="images/Apple-logo.png" style="width:20px; height:20px"> Desain iPhone5 vs iPhone6 <img src="images/Apple-logo.png" style="width:20px; height:20px">
